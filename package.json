    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "react-addons-test-utils": "^15.6.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/API"
